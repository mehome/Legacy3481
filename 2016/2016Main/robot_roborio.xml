<Version version="2.5"/>
<Comment comment="Configured for roboRio deployment."/>

<!-- Absolutely do not change QuickLoad to true, only Ryan should do this. -->
<QuickLoad value="false"/>

<Robot>

<!-- All vector 888 assignments -->
<Victors>
<!-- Drive train assignments -->
	<DriveTrain>
		<rightDrive_1 channel="0" reversed="false"/>
		<rightDrive_2 channel="1" reversed="false"/>
		<!--<rightDrive_3 channel="2" reversed="false"/>-->
		<leftDrive_1 channel="2" reversed="false"/>
		<leftDrive_2 channel="3" reversed="false"/>
		<!--<leftDrive_3 channel="5" reversed="false"/>-->
	</DriveTrain>

	<Extra>
		<climber_1 channel="6" reversed="false"/>
		<climber_2 channel="7" reversed="false"/>
	 	<indexerMotor channel="10" reversed="false"/>
		<intakeMotor channel="11" reversed="false"/>
		<shooter_1 channel="8" reversed="true"/>
		<shooter_2 channel="9" reversed="false"/>
	</Extra>	

</Victors>

<!-- Encoder locations -->
<Encoders>
	<!--<driveDrive aChannel="4" bChannel="6"/>-->
	<leftEncoder aChannel="0" bChannel="1"/>
	<rightEncoder aChannel="2" bChannel="3"/>
	<intakeEncoder aChannel="4" bChannel="5"/>
</Encoders>

<!-- Double solenoid locations-->
<Solenoids>
	<gearShift forward="0" reverse="1" default="forward"/>
	<intakeShift forward="2" reverse="3" default="forward"/>
	<shooterShift forward="4" reverse="5" default="forward"/>
	<climberShift forward="6" reverse="7" default="reverse"/>

</Solenoids>

<!--  Digital input/outputs -->
<DI>
	<!-- for CRIO use only <compressorSwitch channel="0"/>-->
</DI>

<DO>

</DO>

<!-- Analog input/outputs -->
<AIO>
	
</AIO>

<Relays>
	<!--for CRIO use only <Compressor relay="1"/>-->
</Relays>

</Robot>


<!-- Control assignments -->
<Controls>
<!-- Polarity assigns direction 1=positive -1=negative -->
<!-- 0 = false 1 = true-->

	<!-- Driver assignments -->
	<Driver>
	
		<!-- 0=Tank, 1=Arcade -->
		<driveType type="0"/>
		
		<slot controllerSlot="0"/>
		
		<!-- fit values are linear=0, quadratic=1, exponential=2, and poly=3 -->
		<drive driveFit="0" power="3"/>
		 
		<!-- Drive acceleration -->
		<acceleration accelerationFactor=".1"/>
		
		<leftPower powerMultiplier="1"/>
		<rightPower powerMultiplier="1"/>
		
		<!-- Tank -->
		<left_drive axis="5" polarity="1" deadZone=".05"/>
		<right_drive axis="1" polarity="1" deadZone=".05"/>
		
		<!-- Arcade -->
		<drivex axis="0" polarity="1" deadZone=".4"/>
		<turning axis="1" polarity="1" deadZone=".4"/>
		
		<!-- Kicker -->
		<left_kicker axis="2" polarity="1" deadZone=".4"/>
		<right_kicker axis="3" polarity="-1" deadZone=".4"/>
		
		<shift_High button="8"/>
		<shift_Low button="7"/>
		
		<reverse button_a="5" button_b="6"/>
		

	</Driver>

	<!-- Operator assignments -->
	<Operator>
		<slot controllerSlot="1"/>
		<intake inButton="2" outButton="1" polarity="1" powerMultiplier="1"/>
		<indexer axis="3" polarity="1" powerMultiplier="1"/>
		<shooter axis="2" polarity="1" powerMultiplier=".75"/>
		<climber upButton="4" downButton="3" polarity="1" powerMultiplier=".5"/>
		
		<climber_shift button ="6"/>
		<intake_shift button ="7"/>
		<shooter_shift button ="8"/>
		
	</Operator>
</Controls>

