<RobotConfig>
<Version version="3.3"/>
<Comment comment="Configured for practice bot roboRio deployment."/>

<!-- Absolutely do not change QuickLoad to true, only Ryan should do this. -->
<QuickLoad value="false"/>

<EnableAuton value="true"/>

<Robot>

<!-- All vector 888 assignments -->
<Victors>
<!-- Drive train assignments -->
	<DriveTrain>
		<rightDrive_1 channel="0" reversed="true"/>
		<rightDrive_2 channel="1" reversed="true"/>
		<!--<rightDrive_3 channel="2" reversed="false"/>-->
		<leftDrive_1 channel="2" reversed="false"/>
		<leftDrive_2 channel="3" reversed="true"/>
		<!--<leftDrive_3 channel="5" reversed="false"/>-->
	</DriveTrain>

	<Extra>
		<climber_1 channel="6" reversed="true"/>
		<climber_2 channel="7" reversed="true"/>
	 	<indexerMotor channel="10" reversed="false"/>
		<intakeMotor channel="11" reversed="false"/>
		<shooter_1 channel="8" reversed="false"/>
		<shooter_2 channel="9" reversed="true"/>
	</Extra>	

</Victors>

<!-- Encoder locations -->
<Encoders>
	<!--<driveDrive aChannel="4" bChannel="6"/>-->
	<!--<leftEncoder aChannel="0" bChannel="1"/>-->
	<!--<rightEncoder aChannel="2" bChannel="3"/>-->
	<shooterEncoder aChannel="0" bChannel="1"/>
</Encoders>

<!-- Double solenoid locations-->
<Solenoids>
	<gearShift forward="0" reverse="1" default="forward"/>
	<intakeShift forward="2" reverse="3" default="off"/>
	<shooterShift forward="4" reverse="5" default="off"/>
	<climberShift forward="6" reverse="7" default="off"/>
</Solenoids>

<!--  Digital input/outputs !!!!NOTE!!!! DI and DO are on the same line ad the Encoders -->
<!--  so DO NOT have matching channel numbers, that allocated drifferent resources in the same spot.-->
<DI>
	<!-- for CRIO use only <compressorSwitch channel="0"/>-->
	<climberLimitUp channel="4"/>
	<climberLimitDown channel="3"/>
	<!--<ultraSonicInput channel="5"/>-->
</DI>

<DO>
	<!--<ultraSonicOutput channel="6"/>-->
	<beaconOne channel="7"/>
	<beaconTwo channel="8"/>
	<beaconOffState channel="9"/>
	<videoLEDRelay channel="12"/>
</DO>

<!-- Analog inputs/outputs -->
<AI>
	<intakePressurePad channel="0"/>
</AI>

<AO>
	
</AO>


<Relays>
	<!--for CRIO use only <Compressor relay="1"/>-->
</Relays>

<Parameters>

<!-- In CM -->
<BatterShootRange min="26" max="31"/>

</Parameters>

</Robot>


<!-- Control assignments -->
<Controls>
<!-- Polarity assigns direction 1=positive -1=negative -->
<!-- 0 = false 1 = true-->

	<!-- Driver assignments -->
	<Driver>
	
		<!-- 0=Tank, 1=Arcade -->
		<driveType type="0"/>
		
		<slot controllerSlot="0"/>
		
		<!-- fit values are linear=0, quadratic=1, exponential=2, and poly=3 -->
		<drive driveFit="2" power="3"/>
		 
		<!-- Drive acceleration -->
		<acceleration accelerationFactor=".1"/>
		
		<leftPower powerMultiplier="1"/>
		<rightPower powerMultiplier="1"/>
		
		<!-- Tank -->
		<left_drive axis="1" polarity="1" deadZone=".05"/>
		<right_drive axis="5" polarity="1" deadZone=".05"/>
		
		<!-- Arcade -->
		<drivex axis="0" polarity="1" deadZone=".4"/>
		<turning axis="1" polarity="1" deadZone=".4"/>
		
		<!-- Kicker -->
		<left_kicker axis="2" polarity="1" deadZone=".4"/>
		<right_kicker axis="3" polarity="-1" deadZone=".4"/>
		
		<shiftGear button="6"/>
		
		<reverse button_a="7" button_b="8"/>
		

	</Driver>

	<!-- Operator assignments -->
	<Operator>
		<slot controllerSlot="1"/>
		<intake inButton="2" outButton="1" polarity="1" powerMultiplier="1"/>
		<indexer inButton="4" outButton="3" polarity="1" powerMultiplier="1"/>
		<!-- <indexer axis="5" deadZone="0" polarity="1" powerMultiplier="1"/> -->
		<shooter axis="3" deadZone="0" polarity="1" powerMultiplier="1"/>
		<climber axis="1" deadZone=".15" polarity="-1" powerMultiplier="1"/>
		
		<climber_shift button ="5"/>
		<intake_shift button ="7"/>
		<shooter_shift button ="6"/>
		
	</Operator>
</Controls>
</RobotConfig>
