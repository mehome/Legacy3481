<UserControl x:Class="BroncBotz_Dashboard.TableControlItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:BroncBotz_Dashboard"
             mc:Ignorable="d" Height="auto" Width="auto">
    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
        <WrapPanel HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
            <Label x:Name="key" Content="Key: " />
            <Label x:Name="value" Content="N/A" />
        </WrapPanel>
    </Grid>
</UserControl>